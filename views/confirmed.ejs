<!doctype html>
<html>
<head>
    <meta charset="utf-8" />
    <link rel="stylesheet" type="text/css" href="/styles/genericHeader.css" />
</head>
<body>
    <header class="dashboard_header">
        <div class="header_logo">
            <h1><a href="/">Quick Join</a></h1>
        </div>
    </header>

    <div class="main">
        <img src="/images/person-check.svg" height="100">
        <% if (message) { %>
            <span><%= message %></span>
        <% } %>
    </div>

    <style>
        .main {
            width:100vw;
            height:100vh;
            display:flex;
            align-items:center;
            justify-content:center;
            flex-direction:column;
        }
    </style>
</body>
</html>
